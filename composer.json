{
    "name": "genie-fintech/crm_notifications",
    "type": "project",
    "description": ":project_name - laravel v10.2.4.",
    "keywords": ["framework", "laravel", ":crm_notifications", "api"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ageekdev/dev-login": "^2.0",
        "carropublic/notifications": "^1.0.0",
        "genie-fintech/laravel-support": "^2.1",
        "genie-fintech/vapor-http-logger": "^1.1",
        "guzzlehttp/guzzle": "^7.5.0",
        "laravel/framework": "^10.10",
        "laravel/octane": "^1.3.10",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "laravel/vapor-cli": "^1.54",
        "laravel/vapor-core": "^2.28",
        "laravel/vapor-ui": "^1.7",
        "league/csv": "^9.0",
        "spatie/laravel-ignition": "^2.0",
        "spiral/roadrunner": "^2.8.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.21",
        "laravel/pint": "^1.10",
        "laravel/sail": "^1.20",
        "mockery/mockery": "^1.5.0",
        "nunomaduro/collision": "^6.4",
        "nunomaduro/larastan": "^2.4",
        "pestphp/pest-plugin-laravel": "^1.4",
        "roave/security-advisories": "dev-latest"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://genie-fintech.repo.repman.io"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "analyse-clear": "vendor/bin/phpstan clear-result-cache",
        "format": "vendor/bin/pint",
        "check": [
            "@php artisan envy:sync --force",
            "@php artisan test",
            "@analyse-clear",
            "@analyse",
            "@format"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
